{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "appName": {
            "value": "YourAppName"
        },
        "repoURL": {
            "value": "https://github.com/YourName/yourRepo.git"
        },
        "branch": {
            "value": "yourBranch"
        },
        "nsgSourceDataConnection": {
            "value": "blob-account-connection-string"
        },
        "outputBinding": {
            "value": "arcsight"
        },
        "arcsightAddress": {
            "value": "192.168.1.1",
            "metadata": {
                "description": "IP address of arcsight server"
            }
        },
        "arcsightPort": {
            "value": "1514"
        },
        "cefLogAccount": {
            "metadata": {
                "description": "storage account into which incoming json and outgoing cef are logged"
            }
        }
    }
}